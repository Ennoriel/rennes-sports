{
	"name": "rennes-sports",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev --port 3001 --open --host",
		"build": "rm -rf build && svelte-kit build",
		"deploy": "npm run build && node gh-pages -d build -t true && rm -rf build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "jest \".*\\.test.ts\"",
		"prepare": "husky install"
	},
	"main": "src/app.html",
	"devDependencies": {
		"@babel/core": "^7.18.2",
		"@sveltejs/adapter-vercel": "^1.0.0-next.56",
		"@sveltejs/kit": "^1.0.0-next.345",
		"@types/bcryptjs": "^2.4.2",
		"@types/cookie": "^0.5.1",
		"@types/jest": "^27.5.1",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/node": "^17.0.35",
		"@types/svelte-leafletjs": "^0.8.1",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
		"@typescript-eslint/parser": "^5.26.0",
		"babel-jest": "^28.1.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"form-data-to-object": "^0.2.0",
		"husky": "^8.0.1",
		"jest": "^28.1.0",
		"jest-environment-jsdom": "^28.1.0",
		"leaflet": "^1.8.0",
		"mongodb": "^4.6.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.7.1",
		"svelte-leafletjs": "^0.9.0",
		"svelte-preprocess": "^4.10.6",
		"svelte-select": "^5.0.0-beta.11",
		"svelte-table": "^0.3.4",
		"ts-jest": "^28.0.3",
		"ts-node": "^10.8.0",
		"tslib": "^2.4.0",
		"typescript": "^4.7.2"
	},
	"type": "module",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cookie": "^0.5.0",
		"jsonwebtoken": "^8.5.1"
	}
}
